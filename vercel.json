{
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs16.x"
    },
    "api/server.js": {
      "runtime": "nodejs16.x",
      "handler": "server/server.js"
    }
  },
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "api/send-email.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/server.js",
      "dest": "api/server.js"
    }
  ]
}
